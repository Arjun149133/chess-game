// schema.prisma
generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["driverAdapters"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id       String   @id @default(uuid())
    username String   @unique
    email    String
    provider Provider @default(Guest)
    password String?
    picture  String?
}

enum Provider {
    Google
    Password
    Guest
}
